            "description": "The version of SharePoint Framework the project will use:",
            "proposals": [
                "1.18.2",
                "1.18.1",
                "1.18",
                "1.17.4",
                "1.17.3",
                "1.17.2",
                "1.17.1",
                "1.17"
            ],
            "default": "latest"
        }
    },
    "platforms": [
        "Any"
    ]
}